{
  "PhoneAsItem": {
    "enabled": true,
    "exportResource": "ava_core",
    "exportFunction": "hasPhoneItem"
  },
  "general": {
    "useResourceIntegration": true,
    "toggleKey": "f1",
    "toggleCommand": "phone",
    "defaultLanguage": "fr"
  },
  "profanityFilter": {
    "enabled": false,
    "badWords": ["esx"]
  },
  "database": {
    "useIdentifierPrefix": true,
    "playerTable": "ava_players",
    "identifierColumn": "license",
    "identifierType": "license",
    "profileQueries": true,
    "phoneNumberColumn": "phone_number"
  },
  "debug": {
    "level": "silly",
    "enabled": true,
    "sentryEnabled": true
  }
}
